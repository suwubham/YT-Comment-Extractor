{
  "name": "Senti-Surfer",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "hello.html",
    "default_icon": {
      "16": "icon.png"
    }
  }
}
